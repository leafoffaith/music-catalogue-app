<%- include('partials/header')%>
    <div class="ui huge header">New Album</div>
        <form class="ui form" action="/albums" method="POST">
            <div class="field">
                <label>Title</label>
                <input type="text" name="catalog[title]" placeholder="title">
            </div>
            <div class="field">
                <label>Artist</label>
                <input type="text" name="catalog[artist]" placeholder="artist">
            </div>
            <div class="field">
                <label>Body</label>
                <input type="text" name="catalog[body]" placeholder="body">
            </div>
            <input class="ui violet big basic button" type="submit">
        </form>
<%- include('partials/footer')%>
