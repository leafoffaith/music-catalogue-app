<%- include('partials/header')%>
    <div class="ui huge header">Edit Album</div>
    <form action="/albums/<%=catalog._id%>?_method=PUT" method="POST">
            <div class="field">
                <label>Title</label>
                <input type="text" name="catalog[title]" value="<%=catalog.title%>" placeholder="title">
            </div>
            <div class="field">
                <label>Artist</label>
                <input type="text" name="catalog[artist]" value="<%=catalog.artist%>" placeholder="artist">
            </div>
            <div class="field">
                <label>Body</label>
                <input type="text" name="catalog[body]" value="<%=catalog.body%>" placeholder="body">
            </div>
            <input class="ui violet big basic button" type="submit">
        </form>
<%- include('partials/footer')%>
